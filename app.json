{
  "expo": {
    "name": "TIMEZONE",
    "slug": "TimeZone",
    "scheme": "timezone",
    "version": "1.1.9",
    "orientation": "portrait",
    "icon": "./assets/TZ.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to add images in trade feature and profile",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to add images in trade feature and profile",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone to record videos",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos to add images in trade feature and profile"
      },
      "bundleIdentifier": "com.app.timezone.dubai",
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/TZ.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.TimeZone",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.INTERNET"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@notifee/react-native"
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "The app accesses your photos to add images in trade feature.",
          "isAccessMediaLocationEnabled": "true"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "495885272460928",
          "clientToken": "cf69f8b1c92383d4061c57db76f48d32",
          "displayName": "TimeZone",
          "advertiserIDCollectionEnabled": "false",
          "autoLogAppEventsEnabled": "false",
          "isAutoInitEnabled": "true"
        }
      ],
      [
        "expo-apple-authentication"
      ],
      [
        "@react-native-firebase/app",
        "@react-native-firebase/messaging"
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "230951ea-14d2-4ec1-aad8-33f7963c6d15"
      }
    }
  }
}